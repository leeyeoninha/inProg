<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.inha.demo.member.dao.MemberMapper">

    <select id="getMemberByName" resultType="com.inha.demo.member.dto.Member">
        select * from member where name = #{name}
    </select>
    
    <select id="getMemberById" resultType="com.inha.demo.member.dto.Member">
        select * from member where id = #{id}
    </select>
    <insert id="insert">
        INSERT INTO member(
            id,passwd,name,age,email
        ) 
        values(
            #{id},#{passwd},#{name},#{age},#{email}
        )
    </insert>
    
    
</mapper>